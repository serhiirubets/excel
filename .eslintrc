{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
  },
  "rules": {
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"],
  "plugins": ["jest"]
}
